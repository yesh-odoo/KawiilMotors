<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="motorcycle_registry_action" model="ir.actions.act_window">
        <field name="name">Registry List</field>
        <field name="res_model">motorcycle.registry</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create the first Registry
            </p>
        </field>
    </record>
    <menuitem name="Kawaii Motorcycle"
              id="menu_academy"
              sequence="1"
              web_icon="motorcycle_registry,static/description/icon.png">
        <menuitem id="menu_registry_sub" name="Motor Cycle" sequence="1">
            <menuitem id="menu_registry_motorcycle" name="Motorcycles" sequence="2" action="motorcycle_registry_action"/>
        </menuitem>
    </menuitem>          
</odoo>